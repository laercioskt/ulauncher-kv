{
  "required_api_version": "^2.0.0",
  "name": "Kv",
  "description": "Kv extension",
  "developer_name": "La√©rcio Pereira",
  "icon": "images/icon.svg",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "kv_kw",
      "type": "keyword",
      "name": "Kv",
      "default_value": "kv"
    },
    {
      "id": "kv_db_path",
      "type": "input",
      "name": "DB Path",
      "description": "Keep DEFAULT to persist in /home/`user`/.kv.db or specify the whole path, for exemplo, /home/myuser/.my-kv-db-path.db",
      "default_value": "DEFAULT"
    }
  ]
}
